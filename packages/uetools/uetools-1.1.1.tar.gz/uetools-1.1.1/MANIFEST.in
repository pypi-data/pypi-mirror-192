global-exclude *.pyc
global-exclude __pycache__

recursive-include uetools/* *.yaml
recursive-include uetools *.Target
recursive-include uetools *.ini
recursive-include uetools *.py

graft uetools/templates
graft uetools/commands/plugin/templates
graft uetools/commands/project/templates
graft uetools/commands/editor/templates

exclude tox.ini
exclude Makefile
exclude .pylintrc

{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://github.com/CaptorAB/OpenSeries/blob/master/openseries/openseries.json",
  "title": "A schema for timeseries.",
  "description": "",
  "type": "object",
  "required": [
    "_id",
    "instrumentId",
    "local_ccy",
    "name",
    "currency",
    "valuetype",
    "dates",
    "values"
  ],
  "properties": {
    "_id": {
      "type": "string",
      "pattern": "^$|([a-f\\d]{24})"
    },
    "instrumentId": {
      "type": "string",
      "pattern": "^$|([a-f\\d]{24})"
    },
    "local_ccy": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    },
    "currency": {
      "type": "string",
      "pattern": "^[A-Z]{3}$"
    },
    "isin": {
      "type": [
        "string",
        "null"
      ],
      "pattern": "^$|([A-Z]{2})([\\dA-Z]{9})(\\d)$"
    },
    "valuetype": {
      "type": "string"
    },
    "dates": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "string",
        "format": "date",
        "pattern": "^((18|19|20|21|22)\\d\\d)-(0[1-9]|1[012])-(0[1-9]|[12]\\d|3[01])$"
      }
    },
    "values": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "number"
      }
    }
  },
  "additionalProperties": false
}